@model IEnumerable<TestCSharp.Models.User>

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewData["Title"]</title>
    <link rel="stylesheet" href="~/css/site.css" /> 
</head>
<body>
    <h1>@ViewData["Title"]</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Email</th>
                <th>Lastname</th>
                <th>Firstname</th>
                <th>Middlename</th>
                <th>Plant</th>
                <th>Department</th>
                <th>Position</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var user in Model)
            {
                <tr>
                    <td>@user.Email</td>
                    <td>@user.Lastname</td>
                    <td>@user.Firstname</td>
                    <td>@user.Middlename</td>
                    <td>@user.Plant</td>
                    <td>@user.Department</td>
                    <td>@user.Position</td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>
